.tab_content_wrapper{
  .tab_content{
    @include media-query($medium-up){ &:not(.active){ display: none; } }
    &, & p{ color:#7a7a7a; }
    &.rte {
      strong{ color:$comman-color; }
      a:not(.btn) {      
        text-decoration: underline;
      }
    }
    @include media-query($small){
      &:not(.only_content){ text-align: left; border-color: #f0f0f0; background-color: #f8f8f8; }
      &.only_content .inner_tab_content{ padding-left:0; padding-right:0; }
      max-width:100%; border-radius: 5px; -webkit-transition: background .2s ease-in-out; -o-transition: background .2s ease-in-out; transition: background .2s ease-in-out; margin-bottom:20px; font-size:16px;      
      .tab_content_title{ border-bottom: 1px solid #f0f0f0; padding: 14px 20px; padding-left: 28px; position: relative; line-height: 100%;  cursor:pointer; background-color: #f8f8f8; font-size: 112.5%;
        .icon{ position: absolute; top: 50%; -webkit-transform: translateY(-50%); -ms-transform: translateY(-50%); transform: translateY(-50%); left: 8px;
          width:12px; height:12px;
          &:before, &:after { content: ''; display: block; position: absolute;
            -webkit-box-sizing: border-box; box-sizing: border-box;
            border-color: #666; -webkit-transition: all .2s ease-in-out;
            -o-transition: all .2s ease-in-out; transition: all .2s ease-in-out;
          }
          &:before{ left: 0; right: 0; top: 50%;
            -webkit-transform: translateY(-50%); -ms-transform: translateY(-50%); transform: translateY(-50%);
            border-style: solid; border-width: 2px 0 0 0; 
          }
          &:after{ left: 50%; bottom: 0; top: 0; -webkit-transform: translateX(-50%); -ms-transform: translateX(-50%); transform: translateX(-50%);
            border-style: solid; border-width: 0 0 0 2px;
          }
        }
      }
      .inner_tab_content{ display:none; padding: 14px 20px; }
      &.active{
        .tab_content_title .icon:after{ display: none; }
        .inner_tab_content{ display:block; }  
      }
    }
  }
  &:after{
    content:'';
    display: block;
    max-width: 70.1%;
    border-top: 4px dotted #00a1ae;
    margin: 8.51190vw auto 0;    
  }
