.breadcrumbs__item a{font-family: $font-stack-body;color: #acacac!important;text-transform:uppercase!important;    font-weight: bold!important;font-size:13px!important;}

/* product page recharge subscription app css starts from here  */
.rc-option__discount,.rc-option__price{display:none!important;}
.rc-radio{
  padding:0!important;
  background:transparent!important;
  margin: 20px 0;
}
.rc-option__text{
  color: #4c4c4e;
  font-family: "Futura PT";
  font-size: 14px;
  font-weight: 700;
  line-height: 14px;
  text-align: center;
  text-transform: uppercase;
}
.rc-template__legacy-radio .rc_widget__option__selector+.rc-selling-plans{
	padding-left: 10px!important;
    padding-bottom: 0!important;
}
.rc_widget__option__plans__dropdown{
  border-radius: 6px;
  border: 2px solid #e7e7e7;
  color: #4c4c4e;
  font-family: "Futura PT";
  font-size: 14px;
  font-weight: 700;
  text-transform: uppercase;
      padding: 15px 40px 15px 15px;
}
.rc-radio .rc_widget__option__selector{padding:12px!important;border-radius:6px;border:2px solid #e7e7e7;background:transparent;display:flex;align-items:center;}
.rc-template__legacy-radio .rc-radio.rc-option__subsave{display:flex;}
.rc-template__legacy-radio .rc-radio__input, .rc-template__legacy-radio .rc-radio__label{display:flex!important}
.rc-option input[type="radio"]{appearance:none;border:2px solid #d3d3d3;width:24px;height:24px;content:none;outline:none;margin:0;display:inline-block;padding:0;min-height:auto;position:relative;border-radius:0;}
.rc-option--active input[type="radio"]::before{position:absolute;color:#00a1ae!important;content:"\00A0\2713\00A0"!important;font-weight:bolder;font-size:13px;left:50%;top:50%;transform:translate(-50%,-50%)}
button#BIS_trigger{width:auto!important;margin-top:10px!important;display:inline-block;}
.wc_review_main_content .wc_review_btn, .wc_review_main_content .wc_review_listing_select select.wc_select_drop{font-size:14px!important;font-weight:500!important;}
.wc_review_form_filed.wc_wf_cls.wc_review_form_review_title{display:none!important;}
.is_color_input:checked + label .swatch-color:before{content:'';border-color:#00a1ae;}
.wc_review_badge_wrap,.wc_product_review_badge .wc_review_badge_inner .wc_review_badge_star{line-height:0!important;}
#rc_login{display:block;width:100%;text-align:center;margin:20px auto 120px!important;}
.article_dark_section .btn{margin-top:45px;text-decoration:none!important;}
div#rc_login h2{letter-spacing:0;}
div#rc_login h3{margin-bottom:35px;}
@media(min-width:768px){
  div#rc_login h3{font-size:30px;}
}
/**** video css ***/
li.pbgb--li.pbgb--even_li.pbgb--video{width:100%!important;border-right:0;}
li.pbgb--li.pbgb--even_li.pbgb--video .pbgb--inner_content{max-width:1100px;margin:0 auto;padding:40px 10px 32px;}
.direction_use{max-width:396px;margin:90px auto 0px;}
.direction_use a{font-size:24px;text-align:center;display:inline-block;line-height:1.4;padding:11px 5px;border-radius:24px;width:100%;background-color:#00a1ae;color:#fff;}
.direction_use a:hover,.cosw_passionfruit i.fa.fa-check{color:#fff;}
table.adp-discount-table,.adp-discount-table td,.adp-discount-table th{border:1px solid black;border-collapse:collapse!important;border-width:thin!important;}
div#appikon-notification-bar,div#appstle_subscription_widget0{display:none!important;}
.adp-vol-wrapper,.as-p-4 span.as-ml-2.as-ext-sm.as-font-medium.as-text-black,.product-single__media-wrapper:not(:first-child){display:none;}
.Appstle .rte h3{margin-top:55px;margin-bottom:0;}
.Appstle .as-mr-1{margin-right:4px;display:flex;}
.Appstle .as-ml-1 button{background:#00a1ae;}
label.appstle_radio_label .appstle_subscribe_save_text{color:#000;padding:0 10px 0 0;}
.as-bg-white li{background:#fff;margin:0;list-style:none;}
.as-mb-3 ul{margin:0;overflow-y:scroll;}
button.as-bg-indigo-600.appstle-menu-item-selected{background:none;color:#000;font-size:25px;line-height:1;margin:25px auto 0;box-shadow:none;border:none;}
@media(min-width:768px){
  button.as-bg-indigo-600.appstle-menu-item-selected{font-size:38px;margin:35px auto;}
}
.app-main__outer .as-bg-white.as-shadow{box-shadow:none;padding:0;}
@media screen and (max-width:991px){
  .as-p-4.as-w-full .as-grid{display:block;}
  .as-grid .as-group.as-cursor-pointer{margin-bottom:20px;}
}
