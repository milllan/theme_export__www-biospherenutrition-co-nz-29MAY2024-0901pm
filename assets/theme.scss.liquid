  @include media-query($medium-up) {
    width: 25px;
    height: 25px;
  }

  &.icon--wide {
    width: 40px;
  }
}


/*================ Spinner Icon ================*/
.icon-spinner {
  @include animation(spin 500ms infinite linear);
}

/*================ Error Icons ================*/
.icon-error {
  fill: $color-error;
