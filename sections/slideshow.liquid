{%- if section.settings.slideshow_height == 'adapt' -%}
  {% comment %}
    'min_aspect_ratio' is the minimum aspect ratio of images shown without
    whitespace when 'slideshow_height' is set to 'adapt'.
    The aspect ratio values for the first image in the slideshow will be used
    unless it is blank, in that case a ratio of 2:1 will be used.
  {% endcomment %}

  {%- assign first_block = section.blocks[0] -%}
  {%- if first_block.settings.image.aspect_ratio == blank -%}
    {%- assign min_aspect_ratio = 2.0 -%}
  {%- else -%}
    {%- assign min_aspect_ratio = first_block.settings.image.aspect_ratio -%}
  {%- endif -%}
  {% assign wrapper_height = 100 | divided_by: min_aspect_ratio %}
{%- endif -%}

{%- assign text_alignments = section.settings.text_alignment | split: ' ' -%}
{%- assign text_horizontal_alignment = text_alignments.first -%}
{%- assign text_vertical_alignment = text_alignments.last | strip -%}

<div data-section-id="{{ section.id }}" data-section-type="slideshow-section">
  {%- if section.blocks.size > 0 -%}
    <div
      id="SlideshowWrapper-{{ section.id }}"
      class="slideshow-wrapper"
      role="region"
      aria-label="slideshow"
      aria-describedby="slideshow-info"
      tabindex="-1"
    >
      <div
        class="slideshow slideshow--{{ section.settings.slideshow_height }}{% if display_controls %} slideshow--display-controls{% endif %}"
        id="Slideshow-{{ section.id }}"
        data-autorotate="{{ section.settings.autorotate }}"
        data-speed="{{ section.settings.autorotate_speed | times: 1000 }}"
        data-adapt-height="{% if section.settings.slideshow_height == 'adapt' %}true{% else %}false{% endif %}"
        data-slide-nav-a11y="{{ 'sections.slideshow.load_slide' | t: slide_number: '[slide_number]' }}"
        {% if section.settings.slideshow_height == 'adapt' %}
          data-min-aspect-ratio="{{ min_aspect_ratio }}" style="height: {{- wrapper_height -}}vw"
        {% endif %}
      >
        {%- for block in section.blocks -%}
          {%- assign block_image = block.settings.image -%}
          <div class="slideshow__slide slideshow__slide--{{ block.id }}" {{ block.shopify_attributes }}>
            {% if block.settings.mobile_btn_link and block.settings.mob_btn_label %}
              <div class="float-button">
                <a href="{{ block.settings.mobile_btn_link }}" class="btn">
                  {{- block.settings.mob_btn_label }}
                  <span> {% render 'button-right-arrow' %}</span>
                </a>
              </div>
            {% endif %}
            {% if block_image == blank %}
              <div class="slideshow__image js">
                <div class="placeholder-background">
                  {% capture current %}{% cycle 1, 2 %}{% endcapture %}
                  {{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
                </div>
                {% if section.settings.show_overlay %}<div class="slideshow__overlay"></div>{% endif %}
              </div>
            {% else %}
              <div
                {% unless block_image.alt == blank %}
                  role="img"
                  aria-label="{{ block_image.alt | escape }}"
                  data-alt="{{ block_image.alt | escape }}"
                {% endunless %}
                class="slideshow__image box ratio-container lazyload{% unless forloop.first %} lazypreload{% endunless %} js  {%if block.settings.mobile_img != blank %}small--hide{% endif %}"
                data-bg="{{ block_image | img_url: '300x300' }}"
                data-bgset="{% include 'bgset', image: block_image %}"
                data-sizes="auto"
                data-parent-fit="contain"
                style="
                  background-position: {{ block.settings.alignment }};
                  background-image: url('{{ block_image | img_url: '300x300' }}');
                "
              >
                {% if section.settings.show_overlay %}<div class="slideshow__overlay"></div>{% endif %}
              </div>

              {% if block.settings.mobile_img != blank %}
                {%- assign block_mobimage = block.settings.mobile_img -%}
                <div
                  {% unless block_mobimage.alt == blank %}
                    role="img"
                    aria-label="{{ block_mobimage.alt | escape }}"
                    data-alt="{{ block_mobimage.alt | escape }}"
                  {% endunless %}
                  class="slideshow__image box ratio-container lazyload{% unless forloop.first %} lazypreload{% endunless %} js medium-up--hide"
                  data-bg="{{ block_mobimage | img_url: '300x300' }}"
                  data-bgset="{% include 'bgset', image: block_mobimage %}"
                  data-sizes="auto"
                  data-parent-fit="contain"
                  style="
                    background-position: {{ block.settings.alignment }};
                    background-image: url('{{ block_mobimage | img_url: '300x300' }}');
                  "
                >
                  {% if section.settings.show_overlay %}<div class="slideshow__overlay"></div>{% endif %}
                </div>
              {% endif %}
            {% endif %}

            <noscript>
              <div
                class="slideshow__image"
                {% if block_image %}
                  {%- unless block_image.alt == blank %}
                    role="img" aria-label="{{ block_image.alt | escape }}"
                  {%- endunless %}
                  style="background-image: url('{{ block_image | img_url: '2048x' }}'); background-position: {{ block.settings.alignment }};"
                {% endif %}
              >
                {% if block_image == blank %}
                  <div class="placeholder-background">
                    {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' }}
                  </div>
                {% endif %}
              </div>
            </noscript>

            <div class="slideshow__text-wrap slideshow__text-wrap--desktop">
              <div class="slideshow__text-content tuc_{{ block.settings.tuc }} slideshow__text-content--vertical-{{ text_vertical_alignment }} text-{{ text_horizontal_alignment }}">
                <div class="page-width">
                  <div class="banner-content">
                    <div class="star_review">
                      <div class="rating">
                        {% if block.settings.rating == 'one' %}
                          <span> {% render 'active_start' %}</span><span> {% render 'deactive_start' %}</span
                          ><span> {% render 'deactive_start' %}</span><span> {% render 'deactive_start' %}</span
                          ><span> {% render 'deactive_start' %}</span>
                        {% elsif block.settings.rating == 'two' %}
                          <span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                          ><span> {% render 'deactive_start' %}</span><span> {% render 'deactive_start' %}</span
                          ><span> {% render 'deactive_start' %}</span>
                        {% elsif block.settings.rating == 'three' %}
                          <span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                          ><span> {% render 'active_start' %}</span><span> {% render 'deactive_start' %}</span
                          ><span> {% render 'deactive_start' %}</span>
                        {% elsif block.settings.rating == 'four' %}
                          <span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                          ><span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                          ><span> {% render 'deactive_start' %}</span>
                        {% else %}
                          <span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                          ><span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                          ><span> {% render 'active_start' %}</span>
                        {% endif %}
                      </div>
                      {% if block.settings['review-label'] != blank %}
                        <p>{{ block.settings['review-label'] }}</p>
                      {% endif %}
                    </div>
                    {% if block.settings.slide_title != blank %}
                      <h2>{{ block.settings.slide_title }}</h2>
                    {% endif %}
                    <div class="inside_img">
                      {% if block.settings.image_good %}
                        <div class="image_div">
                          <img
                            src="{{ block.settings.image_good | img_url:'master' }}"
                            alt="{{ block.settings.image_good | handle }}"
                            title="{{ block.settings.image_good | handle }}" height="179px" width="107px"
                          >
                        </div>
                      {% endif %}
                      {% if block.settings.image_vegan %}
                        <div class="image_div">
                          <img
                            src="{{ block.settings.image_vegan | img_url:'master' }}"
                            alt="{{ block.settings.image_vegan | handle }}"
                            title="{{ block.settings.image_vegan | handle }}" height="179px" width="107px"
                          >
                        </div>
                      {% endif %}
                      {% if block.settings.artificial_color %}
                        <div class="image_div">
                          <img
                            src="{{ block.settings.artificial_color | img_url:'master' }}"
                            alt="{{ block.settings.artificial_color | handle }}"
                            title="{{ block.settings.artificial_color | handle }}" height="179px" width="107px"
                          >
                        </div>
                      {% endif %}
                      {% if block.settings.artificial_sweetness %}
                        <div class="image_div">
                          <img
                            src="{{ block.settings.artificial_sweetness | img_url:'master' }}"
                            alt="{{ block.settings.artificial_sweetness | handle }}"
                            title="{{ block.settings.artificial_sweetness | handle }}" height="179px" width="107px"
                          >
                        </div>
                      {% endif %}
                      {% if block.settings.image_preservative %}
                        <div class="image_div">
                          <img
                            src="{{ block.settings.image_preservative | img_url:'master' }}"
                            alt="{{ block.settings.image_preservative | handle }}"
                            title="{{ block.settings.image_preservative | handle }}" height="179px" width="107px"
                          >
                        </div>
                      {% endif %}
                      {% if block.settings.image_suger %}
                        <div class="image_div">
                          <img
                            src="{{ block.settings.image_suger | img_url:'master' }}"
                            alt="{{ block.settings.image_suger | handle }}"
                            title="{{ block.settings.image_suger | handle }}" height="179px" width="107px"
                          >
                        </div>
                      {% endif %}
                    </div>
                    <div class="btn-with-label">
                      {% if block.settings.button_link != blank and block.settings.button_label != blank %}
                        <a href="{{ block.settings.button_link }}" class="btn">
                          {{ block.settings.button_label }}
                          <span class="rs-arrow">{% render 'white-btn-left-arrow' %} </span>
                        </a>
                      {% endif %}
                      {% if block.settings.sub_txt != blank %}
                        <p class="info_label">{{ block.settings.sub_txt }}</p>
                      {% endif %}
                    </div>
                  </div>

                  {% comment %}
                    {% unless block.settings.slide_title == blank and block.settings.subheading == blank %}
                      <ul class="slideshow__text-content-list">
                        {%- unless block.settings.slide_title == blank -%}
                        {%- assign f_s = '' -%}
                        {%- assign slide_f_s = block.settings.slide_title.size -%}
                        {%- if slide_f_s < 16 -%}
                        	{%- assign f_s = 'x-large-f' -%}
                        {%- elsif  slide_f_s > 15 and slide_f_s < 44 -%}
                        	{%- assign f_s = 'large-f' -%}
                        {%- elsif  slide_f_s > 43 -%}
                        	{%- assign f_s = 'medium-f' -%}
                        {%- else -%}
                        {%- endif -%}
                          <li>
                            <h2 class="h1 {{f_s}} no-ls heading-font mega-title slideshow__title{% if section.settings.text_size == 'large' %} mega-title--large{% endif %}">
                              {{ block.settings.slide_title | escape }}
                            </h2>
                          </li>
                        {%- endunless -%}
                        {%- unless block.settings.subheading == blank -%}
                          <li>
                            <span class="h-border-bottom mega-subtitle slideshow__subtitle slideshow__subtitle--desktop{% if section.settings.text_size == 'large' %} mega-subtitle--large{% endif %}">
                              {{ block.settings.subheading | escape }}
                            </span>
                          </li>
                        {%- endunless -%}
                        {%- capture feature_block -%}
                        {%- for i in (1..4) -%}
                        {%- capture bt -%}fbt{{i}}{%- endcapture -%}
                        {%- if block.settings[bt] != blank -%}
                        <div class="slideshow__fblock">{{ block.settings[bt] }}</div>
                        {%- endif -%}
                        {%- endfor -%}
                        {%- endcapture -%}
                        {%- if feature_block != blank -%}
                        <li class="slideshow__fblock_li--desktop slideshow__fblock_li fblock_{{block.id}} fw-heavy small--hide ">{{ feature_block }}</li>
                        <style type="text/css">
                          {%- if block.settings.fbtc != blank -%}
                          .fblock_{{block.id}}{  color: {{ block.settings.fbtc }};  }
                          {%- endif -%}
                          {%- if block.settings.fbbgc != blank -%}
                          {%- assign bg_o = block.settings.fbbgco | divided_by: 100.00 -%}
                          .fblock_{{block.id}} .slideshow__fblock{ background-color: {{ block.settings.fbbgc | color_modify: 'alpha', bg_o }}; }
                          {%- endif -%}
                        </style>
                        {%- endif -%}
                      </ul>
                    {% endunless %}
                  {% endcomment %}
                  {%- assign show_link_button = false -%}
                  {%- if block.settings.button_label != blank and block.settings.button_link != blank -%}
                    {%- assign show_link_button = true -%}
                  {%- endif -%}
                  {% comment %}
                    {%- if show_link_button -%}
                              <div class="slideshow__btn-wrapper{% if block.settings.slide_title != blank or block.settings.subheading != blank %} slideshow__btn-wrapper--push{% endif %}">
                                <a
                                  href="{{ block.settings.button_link }}"
                                  class="btn {{ block.settings.button_style }} btn-round_{{ block.settings.cricle_b }} slideshow__btn"
                                >
                                  {{ block.settings.button_label | escape }}
                                  {%- include 'icon-chevron-right' -%}
                                </a>
                              </div>
                            {%- endif -%}
                  {% endcomment %}
                </div>
              </div>
            </div>
          </div>
        {%- endfor -%}
      </div>
      <div class="slideshow__controls">
        {%- if section.blocks.size > 1 -%}
          {%- assign arrows_width = section.blocks.size | times: 18 | plus: 115 -%}
          <div
            class="slideshow__arrows"
            style="width: {{- arrows_width -}}px"
          >
            <button
              class="slideshow__arrow slideshow__arrow-left"
              aria-label="{{ 'sections.slideshow.previous_slide' | t }}"
            >
              {% include 'icon-chevron-left' %}
            </button>
            <button
              class="slideshow__arrow slideshow__arrow-right"
              aria-label="{{ 'sections.slideshow.next_slide' | t }}"
            >
              {% include 'icon-chevron-right' %}
            </button>
          </div>
          {%- if section.settings.autorotate -%}
            <button
              type="button"
              class="slideshow__pause"
              data-id="{{ section.id }}"
              aria-live="polite"
              aria-pressed="false"
            >
              <span class="slideshow__pause-stop">
                {% include 'icon-pause' %}
                <span class="icon__fallback-text">{{ 'sections.slideshow.pause_slideshow' | t }}</span>
              </span>
              <span class="slideshow__pause-rotate">
                {% include 'icon-play' %}
                <span class="icon__fallback-text">{{ 'sections.slideshow.rotate_slideshow' | t }}</span>
              </span>
            </button>
          {%- endif -%}
        {%- endif -%}
      </div>
    </div>
    <style>.template-index .slideshow__text-content--mobile{display:block}</style>
    <div class="slideshow__text-wrap slideshow__text-wrap--mobile medium-up--hide">
      {% if section.blocks.size > 1 %}
        <div class="slideshow__arrows slideshow__arrows--mobile">
          <button
            class="slideshow__arrow slideshow__arrow-left"
            aria-label="{{ 'sections.slideshow.previous_slide' | t }}"
          >
            {% include 'icon-chevron-left' %}
          </button>
          <button
            class="slideshow__arrow slideshow__arrow-right"
            aria-label="{{ 'sections.slideshow.next_slide' | t }}"
          >
            {% include 'icon-chevron-right' %}
          </button>
        </div>
      {% endif %}
      {%- for block in section.blocks -%}
        {%- assign show_text = false -%}
        {%- unless block.settings.slide_title == blank and block.settings.subheading == blank -%}
          {%- assign show_text = true -%}
        {%- endunless -%}
        {%- assign show_link_button = false -%}
        {%- if block.settings.button_label != blank and block.settings.button_link != blank -%}
          {%- assign show_link_button = true -%}
        {%- endif -%}
        {%- if show_text or show_link_button -%}
          <div class="slideshow__text-content slideshow__text-content--mobile slideshow__text-content--mobile-{{ forloop.index0 }} text-center">
            <div class="page-width">
              <div class="banner-content">
                <div class="star_review">
                  <div class="rating">
                    {% if block.settings.rating == 'one' %}
                      <span> {% render 'active_start' %}</span><span> {% render 'deactive_start' %}</span
                      ><span> {% render 'deactive_start' %}</span><span> {% render 'deactive_start' %}</span
                      ><span> {% render 'deactive_start' %}</span>
                    {% elsif block.settings.rating == 'two' %}
                      <span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                      ><span> {% render 'deactive_start' %}</span><span> {% render 'deactive_start' %}</span
                      ><span> {% render 'deactive_start' %}</span>
                    {% elsif block.settings.rating == 'three' %}
                      <span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                      ><span> {% render 'active_start' %}</span><span> {% render 'deactive_start' %}</span
                      ><span> {% render 'deactive_start' %}</span>
                    {% elsif block.settings.rating == 'four' %}
                      <span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                      ><span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                      ><span> {% render 'deactive_start' %}</span>
                    {% else %}
                      <span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                      ><span> {% render 'active_start' %}</span><span> {% render 'active_start' %}</span
                      ><span> {% render 'active_start' %}</span>
                    {% endif %}
                  </div>
                  {% if block.settings['review-label'] != blank %}
                    <p>{{ block.settings['review-label'] }}</p>
                  {% endif %}
                </div>
                {% if block.settings.slide_title != blank %}
                  <h2>{{ block.settings.slide_title }}</h2>
                {% endif %}
                <div class="inside_img">
                  {% if block.settings.image_good %}
                    <div class="image_div">
                      <img
                        src="{{ block.settings.image_good | img_url:'master' }}"
                        alt="{{ block.settings.image_good | handle }}"
                        title="{{ block.settings.image_good | handle }}"
                      >
                    </div>
                  {% endif %}
                  {% if block.settings.image_vegan %}
                    <div class="image_div">
                      <img
                        src="{{ block.settings.image_vegan | img_url:'master' }}"
                        alt="{{ block.settings.image_vegan | handle }}"
                        title="{{ block.settings.image_vegan | handle }}"
                      >
                    </div>
                  {% endif %}
                  {% if block.settings.artificial_color %}
                    <div class="image_div">
                      <img
                        src="{{ block.settings.artificial_color | img_url:'master' }}"
                        alt="{{ block.settings.artificial_color | handle }}"
                        title="{{ block.settings.artificial_color | handle }}"
                      >
                    </div>
                  {% endif %}
                  {% if block.settings.artificial_sweetness %}
                    <div class="image_div">
                      <img
                        src="{{ block.settings.artificial_sweetness | img_url:'master' }}"
                        alt="{{ block.settings.artificial_sweetness | handle }}"
                        title="{{ block.settings.artificial_sweetness | handle }}"
                      >
                    </div>
                  {% endif %}
                  {% if block.settings.image_preservative %}
                    <div class="image_div">
                      <img
                        src="{{ block.settings.image_preservative | img_url:'master' }}"
                        alt="{{ block.settings.image_preservative | handle }}"
                        title="{{ block.settings.image_preservative | handle }}"
                      >
                    </div>
                  {% endif %}
                  {% if block.settings.image_suger %}
                    <div class="image_div">
                      <img
                        src="{{ block.settings.image_suger | img_url:'master' }}"
                        alt="{{ block.settings.image_suger | handle }}"
                        title="{{ block.settings.image_suger | handle }}"
                      >
                    </div>
                  {% endif %}
                </div>
                <div class="btn-with-label">
                  {% if block.settings.button_link != blank and block.settings.button_label != blank %}
                    <a href="{{ block.settings.button_link }}" class="btn">
                      {{ block.settings.button_label }}
                      <span class="rs-arrow">{% render 'white-btn-left-arrow' %} </span>
                    </a>
                  {% endif %}
                  {% if block.settings.sub_txt != blank %}
                    <p class="info_label">{{ block.settings.sub_txt }}</p>
                  {% endif %}
                </div>
              </div>

              {% comment %}
                {%- unless block.settings.slide_title == blank -%}
                  {%- assign f_s = '' -%}
                  {%- assign slide_f_s = block.settings.slide_title.size -%}
                  {%- if slide_f_s < 16 -%}
                    {%- assign f_s = 'x-large-f' -%}
                  {%- elsif slide_f_s > 15 and slide_f_s < 44 -%}
                    {%- assign f_s = 'large-f' -%}
                  {%- elsif slide_f_s > 43 -%}
                    {%- assign f_s = 'medium-f' -%}
                  {%- else -%}
                  {%- endif -%}
                  <h2 class="h1 {{f_s}} heading-font no-ls mega-title slideshow__title slideshow__title--mobile{% if section.settings.text_size == 'large' %} mega-title--large{% endif %}">
                    {{ block.settings.slide_title | escape }}
                  </h2>
                {%- endunless -%}
                {%- unless block.settings.subheading == blank -%}
                  <span class="h-border-bottom mega-subtitle slideshow__subtitle slideshow__subtitle--mobile{% if section.settings.text_size == 'large' %} mega-subtitle--large{% endif %}">
                    {{ block.settings.subheading | escape }}
                  </span>
                {%- endunless -%}
                {%- capture feature_block -%}
                        {%- for i in (1..4) -%}
                        {%- capture bt -%}fbt{{i}}{%- endcapture -%}
                        {%- if block.settings[bt] != blank -%}
                        <div class="slideshow__fblock">{{ block.settings[bt] }}</div>
                        {%- endif -%}
                        {%- endfor -%}
                        {%- endcapture -%}
                {%- if feature_block != blank -%}
                  <div class="slideshow__fblock_li--mobile slideshow__fblock_li fblock_{{block.id}} fw-heavy medium-up--hide">
                    {{ feature_block }}
                  </div>
                {%- endif -%}

                {%- if show_link_button -%}
                  <a
                    href="{{ block.settings.button_link }}"
                    class="btn {{ block.settings.button_style }} btn-round_{{ block.settings.cricle_b }} slideshow__btn slideshow__btn--mobile medium-up--hide"
                  >
                    {{ block.settings.button_label | escape }}
                    {%- include 'icon-chevron-right' -%}
                  </a>
                {%- endif -%}
              {% endcomment %}
            </div>
          </div>
        {%- endif -%}
      {%- endfor -%}
    </div>
  {%- endif -%}

  {% if section.blocks.size == 0 %}
    <div class="placeholder-noblocks">
      {{ 'homepage.onboarding.no_content' | t }}
    </div>
  {% endif %}
</div>

{% schema %}
{
  "name": {
    "da": "Diasshow",
    "de": "Slideshow",
    "en": "Slideshow",
    "es": "Diapositivas",
    "fi": "Diaesitys",
    "fr": "Diaporama",
    "hi": "स्लाइडशो",
    "it": "Presentazione",
    "ja": "スライドショー",
    "ko": "슬라이드 쇼",
    "nb": "Lysbildefremvisning",
    "nl": "Diavoorstelling",
    "pt-BR": "Apresentação de slides",
    "pt-PT": "Apresentação de diapositivos",
    "sv": "Bildspel",
    "th": "สไลด์โชว์",
    "zh-CN": "幻灯片",
    "zh-TW": "素材輪播"
  },
  "class": "index-section index-section--flush index-section--slideshow",
  "max_blocks": 6,
  "settings": [
    {
      "type": "select",
      "id": "slideshow_height",
      "label": {
        "da": "Diashøjde",
        "de": "Diahöhe",
        "en": "Slide height",
        "es": "Altura de diapositiva",
        "fi": "Dian korkeus",
        "fr": "Hauteur de la diapositive",
        "hi": "स्लाइड ऊंचाई",
        "it": "Altezza slide",
        "ja": "スライドの高さ",
        "ko": "슬라이드 높이",
        "nb": "Lysbildehøyde",
        "nl": "Diahoogte",
        "pt-BR": "Altura do slide",
