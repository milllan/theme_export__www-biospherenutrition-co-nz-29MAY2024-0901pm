      .landing-ingredients-list table th, .landing-ingredients-list table td {
        border: none;
        text-align: center;
        padding: 5px 5px!important;
    }
     .landing-ingredients-list table {
        text-align: center;
    }
     .landing-ingredients-list table tbody tr:nth-child(odd) {
        background-color: rgba(0, 0, 0, 0.1);
    }
     .landing-ingredients-list table tbody tr:nth-child(odd) td:first-child {
         border-radius: 100px 0 0 100px;
         width: 25%;
        max-width: 25%;
    }
     .landing-ingredients-list table tbody tr:nth-child(odd) td:last-child {
        border-radius: 0 100px 100px 0;
       width: 25%;
        max-width: 25%;
    }
     .landing-ingredients-list table th:first-child, .landing-ingredients-list table th:last-child {
        width: 25%;
        max-width: 25%;
       padding-bottom: 30px!important;
    }
    .ingredients-list-head h2 {
        font-size: 48px;
        color: #384151;
        font-weight: 800;
        letter-spacing: 0.01px;
        text-transform: inherit;
        text-align: center;
        padding-bottom: 25px;
        margin-bottom: 100px;
        position: relative;
    }
     .ingredients-list-head h2:after {
        content: '';
        position: absolute;
        opacity: 0.502;
        width: 100px;
        height: 4px;
        background-color: rgba(56, 65, 81, 0.5);
        bottom: 0;
        left: 50%;
        transform: translateX(-50%);
    }
     .section-landing-ingredients-list {
        padding-top: 100px;
        padding-bottom: 170px;
    }
    .section-landing-ingredients-list .page-width {max-width: 1310px;}
     .ingredients_list_text {
        padding: 15.5px 0;
        line-height: 26px;
        margin: 0 25px;
        font-size: 20px;
        letter-spacing: 1px;
        text-transform: uppercase;
        color: #fff;
        font-weight: bold;
    }
     .landing-ingredients-list table tbody tr td:nth-child(2), .landing-ingredients-list table tr th:nth-child(2) {
         background: {{ product.metafields.custom.product_colour }};
        width: 50%;
        max-width: 50%;
    }
    .landing-ingredients-list table tr th:nth-child(2) {border-radius: 100px 100px 0 0;}
    .landing-ingredients-list table tbody tr:last-child td:nth-child(2) {position: relative;}
    .landing-ingredients-list table tbody tr:last-child td:nth-child(2):before {
      content: '';
      position: absolute;
      width: 100%;
      left: 0;
      height: 85px;
      background: {{ product.metafields.custom.product_colour }};
      bottom: -85px;
      border-radius: 0 0 100px 100px;
  }
  .landing-ingredients-list table tbody tr:nth-child(odd) td:nth-child(2):after {
    content: '';
    position: absolute;
    width: 100%;
    left: 0;
    background: rgba(0, 0, 0, 0.1);
    z-index: 1;
    height: 100%;
    top: 0;
  }
  .landing-ingredients-list table tbody tr td:nth-child(2) {position: relative;z-index: 99;filter: drop-shadow(19.5px 30.88px 22px rgba(0, 0, 0, 0.3));}
  .landing-ingredients-list table tbody tr td svg {vertical-align: middle;}
  .mobile-svg-wrapper {display: none;}
  .landing-ingredients-list .cls-1{opacity: 1;}
  @media screen and (max-width: 991px) {
    .section-landing-ingredients-list{padding-bottom: 100px;}
  }
  @media screen and (max-width: 768px) {
    .ingredients_list_text {font-size: 14px;}
    .landing-ingredients-list table th svg {width: 100%;}
    .landing-ingredients-list table tbody tr:last-child td:nth-child(2):before {    height: 38px;    bottom: -38px;}
    .mobile-svg-wrapper {display: block;}
    .desktop-svg {display: none;}
    .ingredients_list_text {line-height: 15px;}
    .section-landing-ingredients-list {    padding-bottom: 70px;  padding-top: 40px;}
    .ingredients-list-head h2 {    margin-bottom: 40px;}
  }
</style>
