<script>
  document.addEventListener("DOMContentLoaded", function(event) {
    setTimeout(function(){
      setFAQSchema();
    },100);
    });
    function setFAQSchema() {
      var faqData = jQuery("ul.faq-block li.faq-li");
      var faqStructure = [];
      if(faqData) {
          jQuery(faqData).each(function(){
          var current_ele = $(this);
           var title = current_ele.find(".faq-que");


          var desc = current_ele.find(".faq-ans .inner-ans");
            if(desc && title) {
              title = title.text();
              desc = desc.text();
                faqStructure.push({
                 "@type": "Question",
                  "name": title,
                  "acceptedAnswer": {
                    "@type": "Answer",
                    "text": desc
                  }
                });
            }
        });
      }

      if(faqStructure.length) {
        //console.log("FAQ", faqStructure);

        var faqdata = {
                 "@context": "https://schema.org",
                  "@type": "FAQPage",
                  "mainEntity": faqStructure
         };

        var script2 = document.createElement('script');
        script2.setAttribute('type', 'application/ld+json');
        script2.innerHTML = JSON.stringify(faqdata);
        document.head.appendChild(script2);
      }
    }
</script>
