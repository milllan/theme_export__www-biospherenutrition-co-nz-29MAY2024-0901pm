{%- if product.metafields.custom.product_colour != blank -%}
<style>
  .pdp_tabdesc a,.pbgb--li .faq-li .faq-ans,.product-single__meta .pro_subtitle,.pf_content h2,.gq_headdesc *,.bm_headdesc *,.pdp_tabdesc strong,.pdp_tabinner.active h2,.bioavailable_headdesc *{color:{{ product.metafields.custom.product_colour }};}
  .big_slider .slick-next{border-left-color:{{ product.metafields.custom.product_colour }};}
  .big_slider .slick-prev{border-right-color:{{ product.metafields.custom.product_colour }};}
  .pf_desc li::before{border-color:{{ product.metafields.custom.product_colour }};}
  .review_head h2:after,.pdp_btn_wrap .product-form__cart-submit{background:{{ product.metafields.custom.product_colour }};}
</style>
{%- endif -%}
<ul class="product_main_block">
  <li class="pbgb--li pbgb--odd_li product_section">
    <div class="pdp_container">
      <div class="product-template__container pbgb--inner_content"
           id="ProductSection-{{ section.id }}"
           data-section-id="{{ section.id }}"
           data-section-type="product"
           data-enable-history-state="true"
           data-ajax-enabled="{{ settings.enable_ajax }}"
           >
        {% comment %}
        Get first variant, or deep linked one
        {% endcomment %}
        {%- assign current_variant = product.selected_or_first_available_variant -%}
        {%- assign product_image_zoom_size = '1024x1024' -%}
        {%- assign product_image_scale = '2' -%}
        {%- assign enable_image_zoom = section.settings.enable_image_zoom -%}
        {%- assign compare_at_price = current_variant.compare_at_price -%}
        {%- assign price = current_variant.price -%}
  
        {% case 'medium' %}
        {% when 'small' %}
        {%- assign product_media_width = 'medium-up--one-third' -%}
        {%- assign product_description_width = 'medium-up--two-thirds' -%}
        {%- assign height = 345 -%}
        {% when 'medium' %}
        {%- assign product_media_width = 'medium-up--one-half' -%}
        {%- assign product_description_width = 'medium-up--one-half' -%}
        {%- assign height = 530 -%}
        {% when 'large' %}
        {%- assign product_media_width = 'medium-up--two-thirds' -%}
        {%- assign product_description_width = 'medium-up--one-third' -%}
        {%- assign height = 720 -%}
        {% when 'full' %}
        {%- assign product_media_width = '' -%}
        {%- assign product_description_width = '' -%}
        {%- assign height = 1090 -%}
        {%- assign enable_image_zoom = false -%}
        {% endcase %}
  
        <div class="grid grid--no-gutters pdp_grid product-single{% if section.settings.enable_payment_button %} product-single--medium-media{% endif %}">
          <div class="grid__item product-single__media-group{% if section.settings.media_size == 'full' %} product-single__media-group--full{% endif %}" data-product-single-media-group>
            <div class="product-single__media-group-wrapper wc-check">{%- assign featured_media = product.selected_or_first_available_variant.featured_media | default: product.featured_media -%}
              {% if product.media.size > 1 %}
              {% if product.media.size >= 4 %}
              {%- assign enable_thumbnail_slides = true -%}
              {% endif %}
    
              <div class="thumbnails-wrapper{% if enable_thumbnail_slides == true %} thumbnails-slider--active{% endif %}">
                {% if enable_thumbnail_slides == true %}
                <button type="button" class="btn btn--link thumbnails-slider__btn thumbnails-slider__prev thumbnails-slider__prev--{{ section.id }}">
                  <svg xmlns="http://www.w3.org/2000/svg" width="31" height="16" viewBox="0 0 31 16"><defs><style>.cls-1{fill:#4c4c4e;fill-rule: evenodd;opacity: 0.5;}</style></defs><path id="Navigation_Previous_Item_596" data-name="Navigation Previous Item 596" class="cls-1" d="M120.158,284.5L133,273.719l12.849,10.78,2.653-2.989L133,268.5,117.5,281.51Z" transform="translate(-117.5 -268.5)"/></svg>
                  <span class="icon__fallback-text">{{ 'sections.slideshow.previous_slide' | t }}</span>
                </button>
                {% endif %}
                <ul class="product-single__thumbnails product-single__thumbnails-{{ section.id }}">
                  {% for media in product.media %}
                  <li class="product-single__thumbnails-item product-single__thumbnails-item--medium js">
                    <a href="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"
                       class="text-link product-single__thumbnail product-single__thumbnail--{{ section.id }}"
                       data-thumbnail-id="{{ section.id }}-{{ media.id }}"
                       {% if enable_image_zoom %}data-zoom="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"{% endif %}>
    
                      {%- capture thumbnailAlt -%}
                      {%- if media.media_type == 'video' or media.media_type == 'external_video' -%}
                      {{ 'sections.featured_product.video_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                      {%- elsif media.media_type == 'model' -%}
                      {{ 'sections.featured_product.model_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                      {%- else -%}
                      {{ 'sections.featured_product.gallery_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                      {%- endif -%}
                      {%- endcapture -%}
    
                      <img class="product-single__thumbnail-image" src="{{ media.preview_image | img_url: '110x110', scale: 2 }}" alt="{{ thumbnailAlt }}">
                      {%- if media.media_type == 'video' or media.media_type =='external_video' -%}
                      <div class="product-single__thumbnail-badge">
                        {% include 'icon-video-badge-full-color' %}
                      </div>
                      {%- endif -%}
                      {%- if media.media_type == 'model' -%}
                      <div class="product-single__thumbnail-badge">
                        {% include 'icon-3d-badge-full-color' %}
                      </div>
                      {%- endif -%}
                    </a>
                  </li>
                  {% endfor %}
                </ul>
                {% if enable_thumbnail_slides == true %}
                <button type="button" class="btn btn--link thumbnails-slider__btn thumbnails-slider__next thumbnails-slider__next--{{ section.id }}">
                  <svg xmlns="http://www.w3.org/2000/svg" width="31" height="16" viewBox="0 0 31 16"><defs><style>.cls-1{fill:#4c4c4e;fill-rule: evenodd;opacity: 0.5;}</style></defs><path id="Navigation_Previous_Item_596_copy" data-name="Navigation Previous Item 596 copy" class="cls-1" d="M120.158,699.5L133,710.281l12.849-10.78,2.653,2.989L133,715.5,117.5,702.49Z" transform="translate(-117.5 -699.5)"/></svg>
                  <span class="icon__fallback-text">{{ 'sections.slideshow.next_slide' | t }}</span>
                </button>
                {% endif %}
              </div>
              {% endif %}
              
              <div class="big_slider">
              {%- for media in product.media -%}
              {% include 'media', media: media, featured_media: featured_media, height: height, enable_image_zoom: enable_image_zoom, image_zoom_size: product_image_zoom_size, image_scale: product_image_scale %}
              {%- endfor -%}
              </div>
              <noscript>
                {% capture product_image_size %}{{ height }}x{% endcapture %}
                <img src="{{ featured_media | img_url: product_image_size, scale: product_image_scale }}" alt="{{ featured_media.alt }}" id="FeaturedMedia-{{ section.id }}" class="product-featured-media" style="max-width: {{ height }}px;">
              </noscript>
    
              {% assign first_3d_model = product.media | where: "media_type", "model" | first %}
    
              {%- if first_3d_model -%}
              <button
                      aria-label="{{ 'products.product.view_in_space_label' | t }}"
                      class="product-single__view-in-space"
                      data-shopify-xr
                      data-shopify-model3d-id="{{ first_3d_model.id }}"
                      data-shopify-title="{{ product.title | escape }}"
                      data-shopify-xr-hidden
                      >
                {% include 'icon-3d-badge-full-color' %}<span class='product-single__view-in-space-text'>{{ 'products.product.view_in_space' | t }}</span>
              </button>
              {%- endif -%}
            </div>
            {% if product.metafields.custom.perfect_product_title != blank or product.metafields.custom.perfect_product_video != blank or product.metafields.custom.perfect_product_desc != blank %}
             <div class="pf_content">
                {%- if product.metafields.custom.perfect_product_title != blank -%}<h2>{{ product.metafields.custom.perfect_product_title }}</h2>{%- endif -%}
                {%- if product.metafields.custom.perfect_product_video != blank -%}
                <div class="pf_video">        
                  <div class="pf_videoinr">
                    {% assign media = product.metafields.custom.perfect_product_video.value %}
                    {% if media.media_type == 'video' %}
                      {{ media | video_tag: image_size: '1000x', controls: true, autoplay: false }}
                    {% endif %}
                  </div>
                </div>
                {%- endif -%}
                {%- if product.metafields.custom.perfect_product_desc != blank -%}
                <div class="pf_desc">{{ product.metafields.custom.perfect_product_desc | metafield_tag }}</div>
                {%- endif -%}
             </div>
            {%- endif -%}
          </div>
        
  		{%- assign pro_description = product.description -%}
          {%- if pro_description contains '[split_subtitle]' -%}
          {%- assign pro_description = pro_description | replace:'<p>[split_subtitle]</p>','[split_subtitle]' | replace:'<p><span>[split_subtitle]</span></p>','[split_subtitle]' -%}
            {%- assign pro_description = pro_description | split:'[split_subtitle]' -%}
            {%- assign pro_subtitle = pro_description | first -%}
            {%- assign pro_description = pro_description | last -%}
          {%- endif -%}
          
          <div class="grid__item product-single__detail-group">
            <div class="product-single__meta">
  <!--     product review short code         -->
             <div class="pro_review_badge "><div class="wc_product_review_badge" data-handle="{{ product.handle }}" data-product_id="{{ product.id }}"> </div></div>
  <!-- ============================ -->
              <div class="inline-ttl-price">
                <h1 class="product-single__title">{{ product.title }}</h1> 
                 {%- assign money_price = price | money -%}
                <span class="price-item price-item--regular" data-regular-price>
                {%- if product.price_min != product.price_max -%}
                {{ product.price_max | money }}
                {%- else -%}
                {{ money_price }}
                {%- endif -%}          
                </span>
              </div>
              {%- if product.metafields.product.pro_subtitle != blank -%}<div class="pro_subtitle" >{{ product.metafields.product.pro_subtitle }}</div>{%- endif -%}
              
              {%- if pro_description contains '[split_short_detail]' -%}
              {%- assign pro_description = pro_description | replace:'<p>[split_short_detail]</p>','[split_short_detail]' | replace:'<p><span>[split_short_detail]</span></p>','[split_short_detail]' -%}
                {%- assign pro_description = pro_description | split:'[split_short_detail]' -%}
                {%- assign pro_short_detail = pro_description | first -%}
                {%- assign pro_description = pro_description | last -%}
              {%- endif -%}
              
       
              
              <div class="product__price">
                {% include 'product-price', variant: current_variant, show_vendor: section.settings.show_vendor %}
              </div>
  			{% comment %}
              {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
              <div class="product__policies rte" data-product-policies>
                {%- if shop.taxes_included -%}
                {{ 'products.product.include_taxes' | t }}
                {%- endif -%}
                {%- if shop.shipping_policy.body != blank -%}
                {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                {%- endif -%}
              </div>
              {%- endif -%}
              {% endcomment %}
              {%- if product.metafields.product.pro_short_detail != blank -%}<div class="pf_desc pdp_desc pro_font-size">{{ product.metafields.product.pro_short_detail }}</div>{%- endif -%}
  
              {% capture "form_classes" -%}
              product-form product-form-{{ section.id }}
              {%- unless section.settings.show_variant_labels %} product-form--hide-variant-labels {% endunless %}
              {%- if section.settings.enable_payment_button and product.has_only_default_variant %} product-form--payment-button-no-variants {%- endif -%}
              {%- if current_variant.available == false %} product-form--variant-sold-out {%- endif -%}
              {%- endcapture %}
  
              {% form 'product', product, data-productid: product.id, class:form_classes, novalidate: 'novalidate', data-product-form: '', data-productid : product.id %}
              {% unless product.has_only_default_variant %}
              <div class="product-form__controls-group{% if product.available and product.variants.size == 1 %} hide{% endif %}">
                {% for option in product.options_with_values %}
                <div class="selector-wrapper js product-form__item">
                  
                  <label {% if option.name == 'default' %}class="label--hidden" {% endif %}for="SingleOptionSelector-{{ forloop.index0 }}">
                    {{ option.name }}
                  </label>
                  <select class="single-option-selector single-option-selector-{{ section.id }} product-form__input"
                          id="SingleOptionSelector-{{ forloop.index0 }}"
                          data-index="option{{ forloop.index }}"
                          >
                    {% for value in option.values %}
                    <option value="{{ value | escape }}"{% if option.selected_value == value %} selected="selected"{% endif %}>{{ value }}</option>
                    {% endfor %}
                  </select>
                </div>
                {% endfor %}
              </div>
              {% endunless %}
              
              {% if  product.variants.size > 0 and  product.has_only_default_variant != true %}
              	<div class="pdp_swatch">
                {% render 'swatch' for product.options as swatch %}
                </div>
              {% endif %}
  
              <select name="id" data-productid="{{ product.id }}" id="ProductSelect-{{ section.id }}" class="product-form__variants no-js" data-productid="{{product.id}}">
                {% for variant in product.variants %}
                <option value="{{ variant.id }}" data-variat-title="{{ variant.title }}"
                        {%- if variant == current_variant %} selected="selected" {%- endif -%}
                        >
                  {{ variant.title }}  {%- if variant.available == false %} - {{ 'products.product.sold_out' | t }}{% endif %}
                  </option>
                {% endfor %}
              </select>
                {% comment %}
              {% render 'custom-subscription' %}
                {% endcomment %}
              <div class="pdp_btn_wrap">
                <div class="product-form__controls-group product-form__controls-group--submit">
                  <div class="product-form__item product-form__item--submit
                              {%- if section.settings.enable_payment_button %} product-form__item--payment-button {%- endif -%}
                              {%- if product.has_only_default_variant %} product-form__item--no-variants {%- endif -%}"
                       >
                    <button type="submit" name="add"
                            {% unless current_variant.available %} aria-disabled="true"{% endunless %}
                            aria-label="{% unless current_variant.available %}{{ 'products.product.sold_out' | t }}{% else %}{{ 'products.product.add_to_cart' | t }}{% endunless %}"
                            class="btn main_atc_btn product-form__cart-submit{% if section.settings.enable_payment_button %} btn--secondary-accent{% endif %}"
                            {% if settings.enable_ajax %}aria-haspopup="dialog"{% endif %}
                            data-add-to-cart>
                      <span data-add-to-cart-text>
                        {% unless current_variant.available %}
                        <i class="fa fa-shopping-cart" aria-hidden="true"></i> {{ 'products.product.sold_out' | t }}
                        {% else %}
                        {% comment %}  
                        <i class="fa fa-shopping-cart" aria-hidden="true"></i>
                        
                        {% include 'basket-icon' %} 
                            {% endcomment %}
                        {{ 'products.product.add_to_cart' | t }}
                        {% endunless %}
                      </span>
                      <span class="button_mobile_price"> {% include 'product-price', variant: current_variant, show_vendor: section.settings.show_vendor %} </span>
                      <span class="hide" data-loader>
                        {% include 'icon-spinner' %}
                      </span>
                    </button>
                    {% if section.settings.enable_payment_button %}
                    {{ form | payment_button }}
                    {% endif %}
                  </div>
                </div>   

                {% if section.settings.show_quantity_selector %}
                <div class="qty-add-wrapper">
                  <div class="product-form__controls-group">
                    <div class="product-form__item">
                      <label for="Quantity-{{ section.id }}">{{ 'products.product.quantity' | t }}</label>
                      <ul class="QuantityBox"> {{--}} 
                        <li class="minus-box"><span class="minus">-</span></li> {{--}} 
                        <li class="input-box"> {{--}} 
                          <input type="number" id="Quantity-{{ section.id }}" {{ -}} 
                                 name="quantity" value="1" min="1" pattern="[0-9]*" {{ -}} 
                                 class="QuantityInput product-form__input product-form__input--quantity" data-quantity-input readonly=""> {{--}} 
                        </li> {{--}} 
                        <li class="plus-box"><span class="plus">+</span></li> {{--}} 
                      </ul> {{--}} 
                    </div>
                  </div>
                </div>
                {% endif %}
                
              </div>
                {% if section.settings.show_quantity_selector %}
             
              {%- comment -%}
  <!--      additional-checkout-buttons        -->
                {%- if additional_checkout_buttons -%}
                 <div class="product-additional-checkout-buttons"> 
                  <div class="additional-checkout-buttons">{{ content_for_additional_checkout_buttons }}</div>
                 </div> 
                {%- endif -%}
              {%- endcomment -%}
                  
  <!-- ==================================================== -->          
              {% unless current_variant.available %}
              {% if product.metafields.soldout.sold_out_text != blank %}
              <br> <h6>{{product.metafields.soldout.sold_out_text}}</h6>
              {% endif %}
              {% endunless %}
  
              {% endif %}

              {%- comment -%}
              {%- if section.settings.sss and section.settings.ess != blank -%}
              	<div class="shipping_stat fw-medium{% unless current_variant.available %} hide{% endunless %}" data-shipping-statment>{{ section.settings.ess }}</div>
              {%- endif -%}
                
              {% unless product.available %}
              <a href="#" class="BIS_trigger btn" data-product-data="{{ product | json | escape }}">
                NOTIFY ME WHEN AVAILABLE
              </a>
              {% endunless %}
              {% endcomment %} 
                
              <div class="product-form__error-message-wrapper product-form__error-message-wrapper--hidden{% if section.settings.enable_payment_button %} product-form__error-message-wrapper--has-payment-button{% endif %}"
                   data-error-message-wrapper
                   role="alert"
                   >
                <span class="visually-hidden">{{ 'general.accessibility.error' | t }} </span>
                {% include 'icon-error' %}
                <span class="product-form__error-message" data-error-message>{{ 'products.product.quantity_minimum_message' | t }}</span>
              </div>            
              {% endform %}
            </div>

            {%- if section.blocks.size > 0 -%}
            <div class="pdp_benefit">
              {%- for block in section.blocks -%}
                {%- if block.settings.title != blank or block.settings.icon != blank -%}
                <div class="pdp_benefitinr">
                  {%- if block.settings.icon != blank -%}{{ block.settings.icon }}{%- endif -%}
                  {%- if block.settings.title != blank -%}<span>{{ block.settings.title }}</span>{%- endif -%}
                </div>
                {%- endif -%}
              {%- endfor -%}
            </div>
            {%- endif -%}

            <div class="pdp_tab faq-block">
              {% assign productDescription = product.description | split:"<!-- split -->"%}
              {% if  productDescription[1] != '' %}
              <div class="pdp_tabinner faq-li active">
                <div class="pdp_tabheader faq-que">
                  <h2>Description<span class="faq-plunus"></span></h2>
                </div>
                <div class="pdp_tabdesc faq-ans">
                  {{ productDescription[1] }}
                  <div class="grid__item medium-up--one-half direction-use inside_acco">
                      <div class="dire-use-wrapper">
                      <div class="dire-use">
                      <h3>DIRECTIONS OF USE</h3>
                      <div class="dire-use--wrapper pro_font-size">
                      {%- if product.metafields.product.pro_direc_use != blank -%}
                      {{product.metafields.product.pro_direc_use}}
                      {%- endif -%}                
                      </div>
                      {%- if product.metafields.product.show_use_img -%}
                      <div class="dire-use-img">
                        <img class="lazyload" data-src="{{product.metafields.product.direct_use_image}}" alt="{{product.metafields.product.direct_use_image.alt}}" width="auto" height="auto" />
                      </div>{%- endif -%}
                      
                      </div>
                      {% if product.handle == "nitric-oxide" %}
                      <div class="direction_use">
                      <a class="button" href="https://www.biospherenutrition.co.nz/blogs/nitric-oxide/ultimate-guide-to-fighting-high-blood-pressure">Check out our complete guide to lowering blood pressure here.</a>
                      </div>
                      {% endif %}
                      </div>     
                  </div>            
                </div>
              </div>
              {% endif %}
              {%- if product.metafields.product.ingre_detail != blank -%}
              <div class="pdp_tabinner faq-li">
                <div class="pdp_tabheader faq-que">
                  <h2>Ingredients<span class="faq-plunus"></span></h2>
                </div>
                <div class="pdp_tabdesc faq-ans">
                  {%- comment -%}{{product.metafields.product.ingre_detail}}{%- endcomment -%}
                  <div class="ingredient-wrapper">
                    {% if product.metafields.custom.product_ingredients != blank %}
                      {{ product.metafields.custom.product_ingredients }}
                    {% else %}
                      {%- if product.metafields.product.ingre_img -%}<div class="main-in_img">
                          <img data-src="{{product.metafields.product.ingre_img}}" alt="{{product.metafields.product.ingre_img.alt}}" class="lazyload" width="auto" height="auto" />
                      </div>{%- endif -%}
                    {%- endif -%}
                  </div>
                </div>
              </div>
              {%- endif -%}
              {%- if product.metafields.product.pro_direc_use != blank -%}
              <div class="pdp_tabinner faq-li">
                <div class="pdp_tabheader pdp_dir faq-que">
                  <h2>Directions<span class="faq-plunus"></span></h2>
                </div>
                <div class="pdp_tabdesc faq-ans">
                  {{product.metafields.product.pro_direc_use}} 
                </div>
              </div>
              {%- endif -%}
              <div class="pdp_tabinner review_sec">
                <div class="pdp_tabheader faq-que">
                  <h2><a href="#pbgb--review">Reviews<span class="faq-plunus"></span></a></h2>
                </div>
              </div>
            </div>

            {% if product.metafields.custom.perfect_product_title != blank or product.metafields.custom.perfect_product_video != blank or product.metafields.custom.perfect_product_desc != blank %}
             <div class="pf_content">
                {%- if product.metafields.custom.perfect_product_title != blank -%}<h2>{{ product.metafields.custom.perfect_product_title }}</h2>{%- endif -%}
                {%- if product.metafields.custom.perfect_product_video != blank -%}
                <div class="pf_video">        
                  <div class="pf_videoinr">
                    {% assign media = product.metafields.custom.perfect_product_video.value %}
                    {% if media.media_type == 'video' %}
                      {{ media | video_tag: image_size: '1000x', controls: true, autoplay: false }}
                    {% endif %}
                  </div>
                </div>
                {%- endif -%}
                {%- if product.metafields.custom.perfect_product_desc != blank -%}
                <div class="pf_desc">{{ product.metafields.custom.perfect_product_desc | metafield_tag }}</div>
                {%- endif -%}
             </div>
            {%- endif -%}
  
            {%- comment -%}
            Live region for announcing updated price and availability to screen readers
            {%- endcomment -%}
            <p class="visually-hidden" data-product-status
               aria-live="polite"
               role="status"
               ></p>
  
            {%- comment -%}
            Live region for announcing that the product form has been submitted and the
            product is in the process being added to the cart
            {%- endcomment -%}
            <p class="visually-hidden" data-loader-status
               aria-live="assertive"
               role="alert"
               aria-hidden="true"
               >{{ 'products.product.loader_label' | t }}</p>
            
            {% if section.settings.show_share_buttons %}
            {% include 'social-sharing', share_title: product.title, share_permalink: product.url, share_image: product.featured_media %}
            {% endif %}
          </div>
        </div>
      </div>
    </div>
  </li>
{%- if product.metafields.custom.bioavailable_title != blank or product.metafields.custom.bioavailable_desc != blank -%}
<li class="pbgb--li bioavailable_section">
  <div class="pdp_container">
    <div class="bioavailable_inr">
      {%- if product.metafields.custom.bioavailable_title != blank or product.metafields.custom.bioavailable_desc != blank -%}
      <div class="bioavailable_head text-center">
        {%- if product.metafields.custom.bioavailable_title != blank -%}<div class="bioavailable_headtitle">{{ product.metafields.custom.bioavailable_title | metafield_tag }}</div>{%- endif -%}
